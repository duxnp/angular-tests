{
  "projectType": "application",
  "root": "apps/b-cal",
  "sourceRoot": "apps/b-cal/src",
  "prefix": "bc",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/b-cal",
        "index": "apps/b-cal/src/index.html",
        "main": "apps/b-cal/src/main.ts",
        "polyfills": "apps/b-cal/src/polyfills.ts",
        "tsConfig": "apps/b-cal/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "favicon.ico",
            "input": "libs/b-cal/shared/assets/src",
            "output": "./"
          },
          {
            "glob": "**/*",
            "input": "libs/b-cal/shared/assets/src/assets",
            "output": "assets"
          }
        ],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
          "apps/b-cal/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/b-cal/src/environments/environment.ts",
              "with": "apps/b-cal/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "b-cal:build:production"
        },
        "development": {
          "browserTarget": "b-cal:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "b-cal:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/b-cal/src/**/*.ts",
          "apps/b-cal/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/b-cal"
      ],
      "options": {
        "jestConfig": "apps/b-cal/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": [
    "application:beluvian-calendar",
    "scope:beluvian-calendar",
    "type:app"
  ]
}