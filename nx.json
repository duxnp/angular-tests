{
  "npmScope": "ng-tests",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ],
        "parallel": 1
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress",
      "inlineStyle": false,
      "inlineTemplate": false,
      "prefix": "bry",
      "skipTests": true,
      "standaloneConfig": true,
      "strict": true,
      "tags": "application:,scope:,type:app",
      "viewEncapsulation": "None"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest",
      "prefix": "bry",
      "standaloneConfig": true,
      "strict": true,
      "tags": "application:shared,scope:shared,type:feature"
    },
    "@schematics/angular:component": {
      "style": "scss",
      "changeDetection": "OnPush",
      "displayBlock": true,
      "export": false,
      "flat": false,
      "skipImport": true
    },
    "@schematics/angular:directive": {
      "skipImport": true
    },
    "@nrwl/angular:storybook-configuration": {
      "linter": "eslint"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    },
    "@nrwl/angular:scam": {
      "style": "scss",
      "changeDetection": "OnPush",
      "displayBlock": true,
      "inlineScam": true
    },
    "@nrwl/angular:ngrx": {
      "directory": "+state",
      "barrels": true,
      "facade": true,
      "minimal": true,
      "syntax": "creators",
      "useDataPersistence": true
    },
    "@ngneat/spectator:spectator-component": {
      "jest": true
    }
  },
  "defaultProject": "b-cal"
}